<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/dark_gray</item>
        <item name="colorPrimaryDark">@color/dark_gray</item>
        <item name="colorAccent">@color/dark_gray</item>
    </style>


    <style name="AppTheme.Base" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorControlNormal">@color/light_gray</item>
        <item name="colorControlActivated">@color/dark_gray</item>
        <item name="colorControlHighlight">@color/dark_gray</item>
    </style>
</resources>
